[
  {
  gccargs => [
    '-I..',
  ],
  libs => [
    '../libbtree.a',
  ],
  code => '

#include <stdlib.h>
#include <time.h>

#include "td_private.h"

int main
  (int argc, char* argv[])
{
  unsigned b = 0, i = 0, lists[] = {
    0,
    1, 0,
    1, 2, 0,
    2, 1, 0,
    3, 2, 1, 0,
    3, 2, 1, 2, 3, 0,
    1, 2, 3, 4, 5, 0,
    6, 5, 4, 3, 2, 1, 0,
    1, 2, 1, 2, 1, 2, 0,
    2, 1, 2, 1, 2, 1, 0,
    1, 2, 3, 1, 2, 3, 0,
    0, 0
  };

  while (1) {
    if (lists[ i ] == 0 && lists[ i + 1 ] == 0) {
      break;
    }
    if (lists[ i ] == 0) {
      unsigned j, * l = &(lists[ b ]);
      fprintf(stderr, "Unsorted list: ");
      for (j=b; j < i; j++) { fprintf(stderr, "%u ", lists[ j ]); }
      fprintf(stderr, "\n");
      td_qsort(l, (i-b));
      fprintf(stderr, "Sorted list: ");
      for (j=b; j < i; j++) {
        fprintf(stderr, "%u ", lists[ j ]);
        if (j < i-1) {
          if (lists[ j ] > lists[ j + 1 ]) {
            return ~0;
          }
        }
      }
      fprintf(stderr, "\n\n");
      b = i + 1;
    }
    ++i;
  }

  return 0;
}

   ',
  },
